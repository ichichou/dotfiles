[user]
    name = ichichou
    email = flower.sun.rain.silver@gmail.com
[core]
    editor = vim
    pager = less -cm
    quotepath = false
    excludesfile = ~/.gitignore_global
[diff]
    renames = true
[merge]
    ff = false
[pull]
    ff = only
[color]
    ui = auto
[alias]
    a = add
    aa = add --all
    br = branch
    cl = clone
    cm = commit
    cmm = commit -m
    co = checkout
    cob = checkout -b
    d = diff
    fc = fetch
    lg = log
    lgg = log --graph --oneline --decorate
    lgp = log -p
    mg = merge
    pl = pull
    ps = push
    rl = reflog
    s = status --short --branch
    sh = show
    ss = status

