[user]
  name = ichichou
  email = 35998815+ichichou@users.noreply.github.com

[core]
  editor = vim
  pager = less -M
  quotepath = false
  ignorecase = false

[diff]
  renames = true

[fetch]
  prune = true

[merge]
  ff = false

[pull]
  ff = only

[color]
  ui = auto

[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true

[alias]
  a = add
  aa = add --all
  br = branch
  bra = branch --all
  brd = branch --delete
  cl = clone
  cm = commit --verbose
  cma = commit --verbose --all
  cmam = commit --verbose --all --message
  cmm = commit --verbose --message
  co = checkout
  cob = checkout -b
  di = diff
  fe = fetch
  fep = fetch --prune
  lg = log
  lgg = log --graph --oneline --decorate
  lgp = log --patch
  mg = merge
  pl = pull
  ps = push
  psd = push --delete
  psu = push --set-upstream
  rl = reflog
  s = status --short --branch
  sh = show
  ss = status

